{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [
    {
      "src": "/akvaterachodov",
      "dest": "/stranka/akvaterachodov.html"
    },
    {
      "src": "/akvateracheb", 
      "dest": "/stranka/akvateracheb.html"
    },
    {
      "src": "/chodov",
      "dest": "/chodov.html"
    },
    {
      "src": "/cheb",
      "dest": "/cheb.html"
    },
    {
      "src": "/karlovy-vary",
      "dest": "/karlovy-vary.html"
    },
    {
      "src": "/aktuality",
      "dest": "/aktuality.html"
    },
    {
      "src": "/vop",
      "dest": "/vop.html"
    },
    {
      "src": "/podminky-smecka",
      "dest": "/podminky-smecka.html"
    },
    {
      "src": "/registrace-smecka",
      "dest": "/registrace-smecka.html"
    },
    {
      "src": "/gdpr",
      "dest": "/gdpr.html"
    },
    {
      "src": "/dashboard",
      "dest": "/dashboard.html"
    },
    {
      "src": "/test",
      "dest": "/test.html"
    },
    {
      "src": "/website",
      "dest": "/website.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "**/*.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "**/*.css",
      "headers": [
        {
          "key": "Cache-Control", 
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "**/*.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "functions": {
    "router.php": {
      "runtime": "vercel-php@0.6.0"
    }
  }
}
